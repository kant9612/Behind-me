<table class="table text_base">
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td class=""><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id), class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
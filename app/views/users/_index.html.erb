<table class="table table-borderless text_base">
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td class="user_index_pic_box">
          <div class="user_index_pic">
            <%= attachment_image_tag user, :profile_image, size: "75x75", fallback: "no_image.jpg" %>
          </div>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id), class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>